<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:map="clr-namespace:Syncfusion.Maui.Maps;assembly=Syncfusion.Maui.Maps"
             xmlns:local="clr-namespace:GeoJsonMap"
             x:Class="GeoJsonMap.MainPage">
    <ScrollView>
        <VerticalStackLayout>
            <map:SfMaps>
                <map:SfMaps.Layer>
                    <map:MapShapeLayer ShapesSource="{local:ImageResource GeoJsonMap.ShapeFiles.world1.shp}">
                    </map:MapShapeLayer>
                </map:SfMaps.Layer>
            </map:SfMaps>

            <map:SfMaps>
                <map:SfMaps.Layer>
                    <map:MapShapeLayer ShapesSource="{local:ImageResource GeoJsonMap.ShapeFiles.world-map.json}">
                    </map:MapShapeLayer>
                </map:SfMaps.Layer>
            </map:SfMaps>
        </VerticalStackLayout>
    </ScrollView>
</ContentPage>
